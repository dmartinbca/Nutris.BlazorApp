@namespace NutrisBlazor.Components.Shared

<link href="css/loading.css" rel="stylesheet" />

<div class="loading-overlay">
    <div class="loading-container">
        <div class="spinner-wrapper @(SpinnerSize)">
            @if (UseBase64 && !string.IsNullOrEmpty(Base64Image))
            {
                @* Imagen Base64 *@
                <div class="center-logo-base64"
                     style="background-image: url(@Base64Image);">
                </div>
            }
            else if (!string.IsNullOrEmpty(ImageSrc))
            {
                @* Imagen desde URL *@
                <img src="@ImageSrc"
                     alt="Loading"
                     class="center-logo" />
            }
            else
            {
                @* Fallback: Logo con inicial *@
                <div class="logo-initial">N</div>
            }
            <div class="spinner-circle" />
        </div>
        <p class="loading-text mt-3">@LoadingText</p>
    </div>
</div>

@code {
    [Parameter] public string LoadingText { get; set; } = "Cargando Datos, por favor espere...";
    [Parameter] public string ImageSrc { get; set; } = "img/logo-nutris.jpg";
    [Parameter] public bool UseBase64 { get; set; } = true;
    [Parameter] public string? Base64Image { get; set; }
    [Parameter] public string SpinnerSize { get; set; } = "large"; // "", "small", "large"

    protected override void OnInitialized()
    {
        // Si quieres usar Base64 por defecto, conviértelo aquí
        // Ejemplo de logo Base64 pequeño (reemplazar con tu logo real)
        if (UseBase64 && string.IsNullOrEmpty(Base64Image))
        {
            // Este es un ejemplo, reemplaza con tu logo real convertido a Base64
            Base64Image = "data:image/jpeg;base64,";
        }
    }
}

<style>
    /* Estilos específicos inline para asegurar que funcionen */
    .logo-initial {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 60px;
        height: 60px;
        background: linear-gradient(135deg, #2F5496 0%, #4169E1 100%);
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        color: white;
        font-size: 24px;
        font-weight: bold;
        font-family: 'MuseoSemiBold', 'Raleway', sans-serif;
        opacity: 0;
        animation: logoFadeIn 0.8s ease-in 0.3s forwards;
        visibility: hidden;
        animation-fill-mode: both;
    }
</style>